{{navbar-header}}

<div class="collapse navbar-collapse" id="nav-collapse">
  <ul class="nav navbar-nav">
    {{#if isAuthenticated}}
      <li>
        {{#link-to "themes"}}My Themes{{/link-to}}
      </li>
      <li>
        {{#link-to "export" theme=model}}Export{{/link-to}}
      </li>
    {{/if}}
  </ul>
  <ul class="nav navbar-nav navbar-right">
    {{#if isAuthenticated}}
      <li>
        {{#link-to "change-password"}}Change Password{{/link-to}}
      </li>
      <li><a href="#" {{action "signOut"}}>Sign Out</a></li>
    {{else}}
      <li>
        {{#link-to "sign-up"}}Sign Up{{/link-to}}
      </li>
      <li>
        {{#link-to "sign-in"}}Sign In{{/link-to}}
      </li>
    {{/if}}
  </ul>
</div>
